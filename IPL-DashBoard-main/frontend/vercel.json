{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "rewrites": [
    {
      "source": "/api/v1/match/:path*",
      "destination": "https://ipl-dashboard-3.onrender.com/match/:path*"
    },
    {
      "source": "/api/match/:path*",
      "destination": "https://ipl-dashboard-3.onrender.com/match/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://ipl-dashboard-3.onrender.com/api/:path*"
    }
  ]
}
